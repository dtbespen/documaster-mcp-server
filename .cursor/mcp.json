{
  "mcpServers": {
    "documaster-mcp-server": {
      "command": "/Users/espennergaard/.nvm/versions/node/v22.15.0/bin/node",
      "args": [
        "/Users/espennergaard/Documents/Prosjekter/Neo/documaster-mcp-server/dist/index.js"
      ],
      "transport": {
        "type": "stdio"
      },
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "DOCUMASTER_CLIENT_ID": "${DOCUMASTER_CLIENT_ID}",
        "DOCUMASTER_CLIENT_SECRET": "${DOCUMASTER_CLIENT_SECRET}",
        "DOCUMASTER_TOKEN_URL": "${DOCUMASTER_TOKEN_URL}",
        "DOCUMASTER_API_BASE_URL": "${DOCUMASTER_API_BASE_URL}",
        "DOCUMASTER_SCOPE": "${DOCUMASTER_SCOPE}"
      }
    },
    "task-master-ai": {
      "command": "npx",
      "args": [
        "-y",
        "--package=task-master-ai",
        "task-master-ai"
      ],
      "env": {
        "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
        "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
        "XAI_API_KEY": "${XAI_API_KEY}",
        "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
        "MISTRAL_API_KEY": "${MISTRAL_API_KEY}",
        "AZURE_OPENAI_API_KEY": "${AZURE_OPENAI_API_KEY}",
        "OLLAMA_API_KEY": "${OLLAMA_API_KEY}"
      }
    }
  }
}